{
  "expo": {
    "name": "Revogue",
    "slug": "Revogue",
    "scheme": "Revogue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "/assets/home/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"]
  }
}
